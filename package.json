{
  "private": true,
  "name": "SAT-ACT_Right_PrepBot",
  "version": "0.0.1",
  "description": "Its for the children, putting them in the best possible position to succeed and get into their dream school.",
  "scripts": {
    "clean": "rm -rf build/",
    "lint": "eslint . --debug --ignore-path .gitignore && csscomb -l -v client/",
    "fix": "eslint . --fix --ignore-path .gitignore && csscomb client/",
    "unit": "NODE_PATH=. mocha --compilers js:babel-register --recursive",
    "coverage": "NODE_PATH=. istanbul cover _mocha -- --compilers js:babel-register --recursive",
    "coveralls": "npm run coverage && cat coverage/lcov.info | coveralls",
    "test": "npm-run-all lint unit",
    "ci-test": "npm-run-all lint coveralls",
    "copy-public": "mkdir -p build/ && cp -r client/public/ build/public/",
    "build:server": "webpack --config webpack.server.config.js --progress",
    "build:client": "npm run copy-public && webpack --config webpack.client.config.js --progress",
    "build": "npm-run-all build:client build:server",
    "watch:client": "webpack --config webpack.client.config.js --progress --watch",
    "watch:server": "webpack --config webpack.server.config.js --progress --watch",
    "watch:server-bundle": "nodemon build/server.js",
    "watch": "npm-run-all --parallel watch:client watch:server watch:server-bundle",
    "set-db": "node build/db.js",
    "dev": "npm install && npm-run-all test clean build set-db watch:server-bundle",
    "prod:build": "npm install && NODE_ENV=production npm-run-all test clean build && npm prune --production",
    "prod": "npm run prod:build && npm run set-db; pm2 start build/server.js -i 0 -l /var/log/node/ || pm2 reload all",
    "deploy": "npm install shipit-cli shipit-deploy && shipit production deploy"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.2.0",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.1",
    "chai": "^3.5.0",
    "connect-redis": "^3.2.0",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.1",
    "coveralls": "^2.11.6",
    "crypto": "0.0.3",
    "css-loader": "^0.23.1",
    "csscomb": "^3.1.8",
    "debug": "^2.4.5",
    "eslint": "^2.1.0",
    "eslint-plugin-react": "^4.1.0",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.8.5",
    "form-data": "^1.0.0-rc3",
    "hiredis": "^0.5.0",
    "http-status": "^0.2.0",
    "image-webpack-loader": "^1.6.2",
    "istanbul": "^v1.0.0-alpha",
    "json-loader": "^0.5.4",
    "minimatch": "^3.0.3",
    "morgan": "^1.8.1",
    "mocha": "^2.4.5",
    "mongodb": "^2.2.33",
    "mongoose": "4.11.0",
    "mongoose-unique-validator": "1.0.6",
    "nock": "^7.0.2",
    "node-fetch": "^1.3.3",
    "node-loader": "^0.6.0",
    "node-polyglot": "^1.0.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.8.1",
    "npm-run-all": "^1.4.0",
    "null-loader": "^0.1.1",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.4.0",
    "passport-http": "^0.3.0",
    "passport-auth0": "^0.6.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^0.8.0",
    "pre-commit": "^1.1.2",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-router": "^2.0.0",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.0",
    "serve-favicon": "^2.4.0",
    "shipit-cli": "^1.4.1",
    "shipit-deploy": "^2.1.3",
    "sinon": "2.0.0-pre",
    "style-loader": "^0.13.2",
    "util": "^0.10.3",
    "url-loader": "^0.5.7",
    "uuid": "^3.0.1",
    "webpack": "^2.2.1",
    "whatwg-fetch": "^0.11.0"
  },
  "dependencies": {
    "foundation-sites": "^6.4.3",
    "jquery": ">=3.0.0",
    "what-input": "^4.1.3",
    "motion-ui": "~1.2.2"
  }
}
